    "fuse.js": "^6.6.2",
    "ical-generator": "^3.6.1",
    "solid-js": "^1.7.6",
    "tailwindcss": "^3.3.2",
    "urlpattern-polyfill": "^6.0.2"
  },
  "devDependencies": {
    "@package/scripts": "workspace:^",
