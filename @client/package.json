{
  "name": "@lectures.london/client",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "pnpm tsc --noEmit && pnpm worker && env-cmd --silent -- astro build && mkdir build/functions || true",
